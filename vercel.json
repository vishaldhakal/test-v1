{
   "rewrites": [
       {
           "source": "/(.*)",
           "destination": "/api/index.js"
       }
   ],
   "functions": {
       "api/index.js": {
          "includeFiles": [
            "dist/client/**",
            "dist/server/**",
            "dist/server/entry-server.js"
          ]
       }
   }
}